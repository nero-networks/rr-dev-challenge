{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "tsconfig": "tsconfig.jest.json"
      }
    ]
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!db-vendo-client/)"
  ],
  "globals": {}
}
